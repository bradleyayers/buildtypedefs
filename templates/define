<<in {item, name, static}>>

<<do item.name = name>>

<<t "\t">>

<<if item.type == "Function">>
  <<if static>>static <</if>>
  <<if /\.constructor$/.test(item.id)>>
    <<do item.name = "constructor">>
  <</if>>
  <<fntype item>>
<<else>>
  <<t name>><<if item.type>><<type item>><</if>>;
<</if>>
<<t "\n">>

<<for name, prop in item.properties || {}>>
  <<define {item: prop, name: name}>>
<</for>>

<<for param item.params || []>>
  <<define {item: param, name: param.name}>>
<</for>>

<<if item.returns>>
  <<define {item: item.returns, name: "returns"}>>
<</if>>
